# Part One- Arithmetic
.data
varZ: .word 4
.text

main: 

	addi a0, x0, 15
	addi a1, x0, 10
	addi a2, x0, 5
	addi a3, x0, 2
	addi a4, x0, 18
	addi a5, x0, -3
	addi a6, x0, 0
	
	sub t0, a0, a1
	mul t1, a2, a3
	sub t2, a4, a5
	div t3, a0, a2
	
	add t0, t0, t1
	add t0, t0, t2
	sub a6, t0, t3
	
	sw a6, varZ, t4
	
